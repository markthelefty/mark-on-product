---
layout: nil
---

<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
<title>{{ site.title }}</title>
<icon>https://res.cloudinary.com/dbrkuvff5/image/upload/v1534497637/assets/favicon-32x32.png</icon>
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{ site.url }}/feed.xml</id>
<link type="text/html" href="{{ site.url }}" rel="alternate"/>

{% for post in site.posts limit:10%}
<entry>
<published>{{ post.date | date_to_xmlschema }}</published>
<updated>{{ post.date | date_to_xmlschema }}</updated>
<title>{{ post.title }}</title>
<content type="html"><img alt="Article Image" src="https://res.cloudinary.com/dbrkuvff5/image/upload/f_auto/c_scale,q_auto:best,w_845/{{ page.image | xml_escape }}"/>{{ post.content | xml_escape }}</content>
<link rel="alternate" type="text/html" href="{{ site.url }}{{ post.url }}"/>
<id>{{ site.url }}{{ post.id }}</id>
<author>
<name>{{ site.author.name }}</name>
</author>
</entry>
{% endfor %}

</feed>
